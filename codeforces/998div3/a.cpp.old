#include<iostream>
#include<vector>

using namespace std;

void solve(){
	int a,b,c,d;
	cin >> a >> b >> c >> d;
	if(a+b+b==c && c+c-b==d && a+b+c==d){
		cout << 3 << endl;
	}
	else if(a+b+b==c || c+c-b==d){
		cout << 2 << endl;
	}
	else {
		cout << 1 << endl;
	}
}

int main(){
	int t;
	cin >> t;
	for(;t--;)solve();
}
